<%= link_to "修改管理员", new_admin_manager_path, class: "btn btn-xs btn-info pull-right" %>
<h4>管理员列表</h4>
			<table class="table table-stripped table-bordered table-hover datatable">
				<thead>
					<tr>
		        <th width="5%">ID</th> 
		        <th width="25%">帐号</th> 
		        <th width="25%">角色</th> 
		        <th width="25%">备注</th> 
		        <th width="20%">操作</th> 
					</tr>
				</thead>
				<tbody>
				<% @managers.each do |manager| %>
				<tr>
					<td><%= manager.id %></td>
					<td><%= manager.username %></td>
					<td><%= manager.role %></td>
					<td><%= manager.remark %></td>
					<td>
					<%= link_to("修改", edit_admin_manager_path(manager), class: "btn btn-warning btn-xs") %>
					<%= link_to("删除", admin_manager_path(manager), method: :delete, class: "btn btn-danger btn-xs")%>
					</td>
				</tr>
				<% end %>
				</tbody>
</table>