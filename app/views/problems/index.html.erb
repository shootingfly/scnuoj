<table class="table tabe-stripped table-bordered table-hover datatable"> 
	<thead> 
	 <tr> 
	   <th width="6%">题号</th> 
	   <th width="26%">标题</th> 
	   <th width="8%">AC数</th> 
	   <th width="8%">提交数</th> 
	 </tr> 
	</thead> 
	<tbody>
    <% @problems.each do |problem| %>
      <tr>
        <td><%= problem.problem_id %></td>  
        <td>
        <%= link_to problem.title, problem_path(problem.problem_id) %></td>  
        <td><%= problem.ac %></td>  
        <td><%= problem.submit %></td>  
      </tr>
    <% end %>
  </tbody> 
</table>